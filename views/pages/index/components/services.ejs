<section id="ts-service-area" class="ts-service-area pb-0">
  <div class="container">
    <div class="row text-center">
      <div class="col-12">
        <h2 class="section-title">NÃ³s somos especialistas</h2>
        <h3 class="section-sub-title">No que fazemos</h3>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-4">
        <% dbData.services.forEach((item, i)=> { %>
          <% if(i >= 4) return; %>
            <div data-aos="fade-right" class="ts-service-box d-flex">
              <div class="ts-service-box-img">
                <img loading="lazy" width="60" height="60" src="<%= item.icon %>" alt="service-icon">
              </div>
              <div class="ts-service-box-info">
                <h3 class="service-box-title"><a href="/servicos/<%= item.id %>">
                    <%= item.title %>
                  </a></h3>
                <p>
                  <%= item.desc %>
                </p>
              </div>
            </div>
          <% }); %>
      </div>
      <div id="imgCenter" data-aos="fade-up" class="col-lg-4 text-center">
        <img loading="lazy" class="img-fluid"
          src="https://cdn.discordapp.com/attachments/778691566168440854/1003739260102398002/Sem_titulo-1.svg"
          alt="service-avater-image">
      </div>
      <div class="col-lg-4 mt-5 mt-lg-0 mb-4 mb-lg-0">
        <% dbData.services.forEach((item, i)=> { %>
          <% if(i < 4) return; %>
            <div data-aos="fade-left" class="ts-service-box d-flex">
              <div class="ts-service-box-img">
                <img loading="lazy" width="60" height="60" src="<%= item.icon %>" alt="service-icon">
              </div>
              <div class="ts-service-box-info">
                <h3 class="service-box-title">
                  <a href="/servicos/<%= item.id %>">
                    <%= item.title %>
                  </a>
                </h3>
                <p>
                  <%= item.desc %>
                </p>
              </div>
            </div>
          <% }); %>
      </div>
    </div>
  </div>
</section>