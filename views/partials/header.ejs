            <div class="search-block" style="display: none;">
            <label for="search-field" class="w-100 mb-0">
              <input type="text" class="form-control" id="search-field"
                placeholder="Escreva o que você precisa e aperte ENTER">
            </label>
            <span class="search-close">&times;</span>
          </div> 
        -->
        </div>
      </div>
    </header>
    <% if(logMessage.content !==null) { %>
      <script>
        setTimeout(() => {
          notif({
            msg: `
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-bell" viewBox="0 0 16 16">
  <path d="M8 16a2 2 0 0 0 2-2H6a2 2 0 0 0 2 2zM8 1.918l-.797.161A4.002 4.002 0 0 0 4 6c0 .628-.134 2.197-.459 3.742-.16.767-.376 1.566-.663 2.258h10.244c-.287-.692-.502-1.49-.663-2.258C12.134 8.197 12 6.628 12 6a4.002 4.002 0 0 0-3.203-3.92L8 1.917zM14.22 12c.223.447.481.801.78 1H1c.299-.199.557-.553.78-1C2.68 10.2 3 6.88 3 6c0-2.42 1.72-4.44 4.005-4.901a1 1 0 1 1 1.99 0A5.002 5.002 0 0 1 13 6c0 .88.32 4.2 1.22 6z"/>
</svg> <%=logMessage.content%>`,
            type: "<%=logMessage.type%>",
            position: "bottom"
          });
        }, 3000)
      </script>
      <% } %>


        <!-- Modal -->
        <div class="modal fade" id="searchModal" tabindex="-1" role="dialog" aria-labelledby="searchModalLabel"
          aria-hidden="true">
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <h5 class="modal-title" id="searchModalLabel">Pesquisar no site</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
              <div class="modal-body" style="z-index: 999;">
                <form action="/pesquisa" method="get">
                  <input type="text" class="form-control" name="q" aria-describedby="searchHelp"
                    placeholder="Gerador..." required>
                  <hr>
                  <button style="width: 100%;" type="submit" class="btn btn-primary">Pesquisar</button>
                </form>
              </div>
            </div>
          </div>
        </div>