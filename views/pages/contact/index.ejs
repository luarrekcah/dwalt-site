<%- include('../../partials/header.ejs') %>
  <div id="banner-area" class="banner-area" style="background-image:url(images/banner/banner1.jpg)">
    <div class="banner-text">
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <div class="banner-heading">
              <h1 class="banner-title">Contato</h1>
              <nav aria-label="breadcrumb">
                <ol class="breadcrumb justify-content-center">
                  <li class="breadcrumb-item"><a href="/">Página Principal</a></li>
                  <li class="breadcrumb-item"><a href="#">Empresa</a></li>
                  <li class="breadcrumb-item active" aria-current="page">Contato</li>
                </ol>
              </nav>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <section id="main-container" class="main-container">
    <div class="container">
      <div class="row text-center">
        <div class="col-12">
          <h2 class="section-title">Nossos Escritórios</h2>
          <h3 class="section-sub-title">Sede</h3>
        </div>
      </div>
      <div class="row">
        <div class="col-md-4">
          <div class="ts-service-box-bg text-center h-100">
            <span class="ts-service-icon icon-round">
              <i class="fas fa-map-marker-alt mr-0"></i>
            </span>
            <div class="ts-service-box-content">
              <h4>Endereço</h4>
              <p>Av. Ceará, 2042 - Bosque - Rio Branco - AC CEP 69900-379
                / CNPJ: 26.711.744/0001-08</p>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="ts-service-box-bg text-center h-100">
            <span class="ts-service-icon icon-round">
              <i class="fa fa-envelope mr-0"></i>
            </span>
            <div class="ts-service-box-content">
              <h4>E-mail</h4>
              <p><%= dbData.infos.email %></p>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="ts-service-box-bg text-center h-100">
            <span class="ts-service-icon icon-round">
              <i class="fa fa-phone-square mr-0"></i>
            </span>
            <div class="ts-service-box-content">
              <h4>Telefones</h4>
              <p><%= dbData.infos.telefone %></p>
              <p><%= dbData.infos.telefoneTwo %></p>
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12">
          
    <div class="gap-40"></div>
          <h3 class="column-title">Envie nos uma mensagem! (Formulário em desenvolvimento)</h3>
          <form id="contact-form" action="#" method="post" role="form">
            <div class="error-container"></div>
            <div class="row">
              <div class="col-md-4">
                <div class="form-group">
                  <label>Nome</label>
                  <input class="form-control form-control-name" name="name" id="name" placeholder="" type="text"
                    required>
                </div>
              </div>
              <div class="col-md-4">
                <div class="form-group">
                  <label>E-mail</label>
                  <input class="form-control form-control-email" name="email" id="email" placeholder="" type="email"
                    required>
                </div>
              </div>
              <div class="col-md-4">
                <div class="form-group">
                  <label>Subtítulo</label>
                  <input class="form-control form-control-subject" name="subject" id="subject" placeholder="" required>
                </div>
              </div>
            </div>
            <div class="form-group">
              <label>Mensagem</label>
              <textarea class="form-control form-control-message" name="message" id="message" placeholder="" rows="10"
                required></textarea>
            </div>
            <div class="text-right"><br>
              <button class="btn btn-primary solid blank" type="submit">Enviar Mensagem</button>
              <a href="<%= dbData.infos.whatsapp %>" class="btn btn-primary solid blank" type="submit">Abrir WhatsApp</a>
            </div>
          </form>
        </div>
      </div>
    </div>
  </section>
  <%- include('../../partials/footer.ejs') %>